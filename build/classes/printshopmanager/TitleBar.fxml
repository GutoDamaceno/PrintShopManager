<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_RIGHT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="printshopmanager.TitleBarController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <FontAwesomeIcon fx:id="btnMin" fill="#00a2e8" iconName="CIRCLE" onMouseClicked="#min">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIcon>
      <FontAwesomeIcon fx:id="btnExit" fill="#ff3b49" iconName="CIRCLE" onMouseClicked="#close">
         <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
      </FontAwesomeIcon>
   </children>
</HBox>
